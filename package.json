{
    "name": "api-tests-jest-pactum",
    "version": "1.0.0",
    "description": "API testing with JestJS and PactumJS.",
    "main": "index.js",
    "devDependencies": {
        "@types/jest": "^29.5.11",
        "@typescript-eslint/eslint-plugin": "^6.19.1",
        "@typescript-eslint/parser": "^6.19.1",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-prettier": "^5.1.3",
        "prettier": "^3.2.4",
        "rimraf": "^5.0.5",
        "mkdirp": "^3.0.1",
        "eslint": "^8.56.0",
        "jest": "^29.7.0",
        "jest-html-reporters": "^3.1.7",
        "pactum": "^3.6.0",
        "ts-jest": "^29.1.2",
        "typescript": "^5.3.3",
        "@faker-js/faker": "^8.4.0",
        "http-status-codes": "^2.2.0"
    },
    "scripts": {
        "clean": "rimraf ./output && mkdirp ./output",
        "format": "prettier --write \"**/**.[jt]s?(x)\"",
        "verify": "prettier --check \"**/**.[jt]s?(x)\"",
        "eslint": "eslint \"**/**.[jt]s?(x)\" -o output/eslint.html -f html",
        "test": "jest --config ./jest.config.js",
        "ci": "npm run clean && npm run format && npm run verify && npm run eslint && npm run test"
    }
}